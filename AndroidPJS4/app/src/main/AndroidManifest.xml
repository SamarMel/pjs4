    package="com.example.preclarity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_mdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Forum.search.SearchActivity"></activity>
        <activity android:name=".faq.FaqActivity" />
        <activity android:name=".HomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".chat.ConversationActivity" />
        <activity android:name=".ListChatActivity" />
        <activity android:name=".CommentActivity" />
        <activity android:name=".TopicActivity" />
        <activity android:name=".Forum.AccueilForum" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".login.MainActivity" />
    </application>

</manifest>